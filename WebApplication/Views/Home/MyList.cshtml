@using WebApplication.Extensions
@using Vereyon.Web


@Styles.Render("~/AdminLTE/plugins/datatables/css")
@Styles.Render("~/AdminLTE/plugins/datepicker/css")
@Styles.Render("~/AdminLTE/plugins/daterangepicker/css")
@Styles.Render("~/AdminLTE/dist/css")
@Styles.Render("~/AdminLTE/dist/css/skins")

<div class="row">
    <div class="col-xs-12">
        <div class="box">
            <div class="box-header">
                <h3 class="box-title">Consulta Dummy</h3>
            </div>
            <div class="box-body">

                <div class="row">

                    <div class="col-md-3" style="align-items: center !important;">
                        <div class="form-group has-feedback">
                            <label>Fecha inicial:</label>
                            <div class="input-group">
                                <div class="input-group-addon">
                                    <i class="fa fa-calendar"></i>
                                </div>
                                <input type="text" class="form-control" id="from">
                            </div>
                        </div>
                    </div>

                    <div class="col-md-3" style="align-items: center !important;">
                        <div class="form-group has-feedback">
                            <label>Fecha final:</label>
                            <div class="input-group">
                                <div class="input-group-addon">
                                    <i class="fa fa-calendar"></i>
                                </div>
                                <input type="text" class="form-control" id="to">
                            </div>
                        </div>
                    </div>

                    <div class="col-md-6"></div>
                </div>

                <table id="dataTbl" class="table table-bordered table-hover">
                    <thead>
                        <tr>
                            <th class="dt-center"># Ticket</th>
                            <th class="dt-center">Descripcion</th>
                            <th class="dt-center">Proyecto</th>
                            <th class="dt-center">Fecha</th>
                            <th class="dt-center">Usuario Asignado</th>
                            <th class="dt-center">Estatus</th>
                        </tr>
                    </thead>
                </table>
            </div>
        </div>

    </div>
</div>





<script type="text/javascript">
    var urlSample = "@Html.Raw(Url.Action("Sample", "Home"))";
</script>

@section Scripts {
    @Scripts.Render("~/AdminLTE/plugins/datatables/js")
    @Scripts.Render("~/AdminLTE/plugins/momentjs/js")
    @Scripts.Render("~/AdminLTE/plugins/datepicker/js")
    @Scripts.Render("~/Scripts/Home/MyList")
}